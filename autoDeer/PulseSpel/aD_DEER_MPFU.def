
; 4p and 5p DEER exp defs for autoDEER
; 2022 Gunnar Jeschke & Hugo Karas

begin defs 
;
srt  = 4000 * srtu ; shot repetition time, srtu= 1 us
;                  ; 
;
; Pulse lengths
;                        
p0 = 16      ; observer pi pulse length (frq1)
p1 = 16      ; observer pi/2 pulse length (frq1) 
p2 = 16      ; pump pi pulse length (frq2)
pg = 32    ; Integrator Gate Width 

;
; Variable delays
;
d0   = 600     ; field-sweep X-band

d1   = 500     ; tau1 (initial value)
d2   = 2000    ; tau2
d11  = 200     ; tau3
d3   = 80      ; deadtime delay during which no data are acquired
               ; d3 >= 360, if TWT window must be split (single gate time exceeded)
;
; Time increments
;
d30  = 16      ; t time scale increment of dipolar time evolution
d31  = 16      ; time scale increment for nuclear modulation averaging
;
; Counters
;
h    = 20      ; number of shots/loop                     ( counter: I )
n    = 2       ; number of sweeps to accumulate           ( counter: K )
m    = 1       ; number of tau1 averages for nuclear modulation
;                suppression
;
; ### Block of additional variables (Bruker definitions) ###
p3   = 40      ;
p9   = 1000    
;
d4   = 3000    ;
d5   = 400     ;
;
dx   = 0       ; t time scale starting value
dy   = 0       ; t1 time scale starting value
;
;
s    = 300     ; Sweep length (n. of data really taken)   ( counter: X )
t    = 1       ; second time axis sweep length            ( counter: Y )
;
r    = 1       ;
c    = 1       ;
b    = 1       ;
;
;
d6   = 100 us  
end defs

