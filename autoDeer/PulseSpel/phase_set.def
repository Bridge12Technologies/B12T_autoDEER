; This is a PulseSpel Script for automatic tuning
;
;
; Hugo Karas 2022

begin defs

srt  = 6000 * srtu ; Standard for nitroxides at 50K
                    ; 80K: 1000
                    ; 50K: 4000
                    ; 15K: 15000-25000

p0   = 16      ; pi pulses
p1   = 32      ; pi/2 pulses

pg   = 16      ; Integrator Gate Width

d0   = 652     

d1 = 400


h    = 10       ; number of shots/sweep
n    = 1       ; number of sweeps to accumulate 

end defs