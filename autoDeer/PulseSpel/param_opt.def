; This is the pulse spel script for parameter optimization for 4p/5p DEER
; Included here are variable defintion files
;
; Hugo Karas 2021/2022

begin defs

srt  = 4000 * srtu ; Standard for nitroxides at 50K
                    ; 80K: 1000
                    ; 50K: 4000
                    ; 15K: 15000-25000

; Pulse Lengths
p0   = 32      ; pi pulses
p1   = 16      ; pi/2 pulses
pg   = 16      ; Integrator Gate Width

; Pulse Delays
d0   = 652     

d1   = 200     
d2   = 300     

; Time increments
d12  = 100     
d14  = 30  
d16  = 2    ; Nuclear Modulation Averaging

h    = 4       ; number of shots/loop
n    = 2       ; number of sweeps to accumulate 
m   = 4 ; Nuclear modulation loops

w    = 15
p2   = 32 ns   
end defs

